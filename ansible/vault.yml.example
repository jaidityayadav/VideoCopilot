---
# Encrypted secrets - use ansible-vault to encrypt this file
# Command: ansible-vault encrypt vault.yml

# Database
database_url: "postgresql://videocopilot:your_secure_password@postgres:5432/videocopilot?sslmode=require"

# Pinecone
pinecone_api_key: "your_pinecone_api_key_here"
pinecone_index_name: "vidwise-embeddings"
pinecone_environment: "gcp-starter"

# Groq
groq_api_key: "your_groq_api_key_here"

# AWS S3
aws_access_key_id: "your_aws_access_key_here"
aws_secret_access_key: "your_aws_secret_key_here"
aws_region: "us-east-1"
aws_s3_bucket: "videocopilot-storage"

# NextAuth
nextauth_secret: "your_nextauth_secret_here"
nextauth_url: "https://your-domain.com"

# Additional secrets
jwt_secret: "your_jwt_secret_here"
encryption_key: "your_encryption_key_here"

# Ansible SSH authentication
ansible_ssh_password: "your_droplet_root_password_here"